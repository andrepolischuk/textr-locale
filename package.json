{
  "name": "textr-locale",
  "description": "Detect the client locale for Textr",
  "version": "0.1.0",
  "keywords": [
    "client",
    "os",
    "browser",
    "locale",
    "textr"
  ],
  "repository": "andrepolischuk/textr-locale",
  "dependencies": {
    "locale2": "^2.0.3"
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "chokidar-cli": "^1.1.0",
    "rimraf": "^2.5.1",
    "textr": "^0.3.0"
  },
  "main": "index.es5.js",
  "scripts": {
    "test": "ava --require babel-core/register",
    "tdd": "chokidar \"*.js\" --ignore \"*.es5.js\" --command \"npm run test\" --initial",
    "prepublish": "babel index.js --out-file index.es5.js",
    "postpublish": "rimraf index.es5.js && git push --follow-tags"
